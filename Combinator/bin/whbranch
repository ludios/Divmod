#!/usr/bin/python

import sys

from combinator.branchmgr import theBranchManager

if len(sys.argv) > 1:
    whichBranch = sys.argv[1]
else:
    whichBranch = None

for k, v in theBranchManager.getCurrentBranches():
    if whichBranch is not None:
        if k == whichBranch:
            print v
            break
    else:
        print k+":", v
